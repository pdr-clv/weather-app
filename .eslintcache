.styles';\n\nimport { ForecastData } from '../../store/types/forecastTypes';\n\ninterface SecondaryProps extends ForecastData {\n  farengeit: boolean;\n}\n\nconst SecondaryInfo: FC<SecondaryProps> = (props) => {\n  //invoque class LocalTime, and initializing with data.timezone\n  let localTimeZone = 0;\n  if (props) localTimeZone = props.timezone_offset;\n  const localTime = new LocalTime(localTimeZone);\n  let windSpeed: string;\n  let temp_max_translated: string;\n  let temp_min_translated: string;\n  let speedUnits: string;\n  let temp_units: string;\n  if (props.farengeit) {\n    windSpeed = getMilesHour(props.current.wind_speed);\n    temp_max_translated = getFarengeit(props.daily[0].temp.max);\n    temp_min_translated = getFarengeit(props.daily[0].temp.min);\n    speedUnits = 'miles/h';\n    temp_units = 'ºF';\n  } else {\n    windSpeed = getKmHour(props.current.wind_speed);\n    temp_max_translated = getCelsius(props.daily[0].temp.max);\n    temp_min_translated = getCelsius(props.daily[0].temp.min);\n    speedUnits = 'km/h';\n    temp_units = 'ºC';\n  }\n\n  return (\n    <SecondaryContainer wind={props.current.wind_deg}>\n      <div className='secondary-item'>\n        <div className='wind-item'>\n          <h4>\n            WIND <p className='direction'>&#8593;&#8593;</p>\n          </h4>\n          <p>\n            {windSpeed} <span>{speedUnits}</span>\n          </p>\n        </div>\n      </div>\n      <div className='secondary-item'>\n        <h4>HUMIDITY</h4>\n        <p>\n          {props.current.humidity}\n          <span>%</span>\n        </p>\n      </div>\n      <div className='secondary-item'>\n        <h4>SUNRISE</h4>\n        <p className='p-small'>\n          {localTime.getTargetTime(props.daily[0].sunrise)}\n        </p>\n      </div>\n      <div className='secondary-item'>\n        <h4>CLOUDS</h4>\n        <p>\n          {props.current.clouds}\n          <span>%</span>\n        </p>\n      </div>\n      <div className='secondary-item'>\n        <h4>PRESSURE</h4>\n        <p className='p-small'>\n          {props.current.pressure}\n          <span>Pa</span>\n        </p>\n      </div>\n      <div className='secondary-item'>\n        <h4>SUNSET</h4>\n        <p className='p-small'>\n          {localTime.getTargetTime(props.daily[0].sunset)}\n        </p>\n      </div>\n    </SecondaryContainer>\n  );\n};\n\nexport default SecondaryInfo;\n","/home/pdr-clv/react/weather-ts/src/components/secondary-info/secondary-info.styles.tsx",[],"/home/pdr-clv/react/weather-ts/src/components/input-place/input-place.component.tsx",["128"],"/home/pdr-clv/react/weather-ts/src/components/input-place/input-place.styles.tsx",[],"/home/pdr-clv/react/weather-ts/src/store/types/forecastTypes.ts",[],"/home/pdr-clv/react/weather-ts/src/store/types/alertTypes.ts",[],"/home/pdr-clv/react/weather-ts/src/store/actions/forecastActions.ts",[],"/home/pdr-clv/react/weather-ts/src/store/reducers/forecastReducer.ts",[],{"ruleId":"129","replacedBy":"130"},{"ruleId":"131","replacedBy":"132"},{"ruleId":"129","replacedBy":"133"},{"ruleId":"131","replacedBy":"134"},{"ruleId":"135","severity":1,"message":"136","line":32,"column":5,"nodeType":"137","messageId":"138","endLine":32,"endColumn":24},{"ruleId":"135","severity":1,"message":"139","line":33,"column":5,"nodeType":"137","messageId":"138","endLine":33,"endColumn":24},{"ruleId":"135","severity":1,"message":"140","line":35,"column":5,"nodeType":"137","messageId":"138","endLine":35,"endColumn":15},{"ruleId":"135","severity":1,"message":"141","line":43,"column":34,"nodeType":"137","messageId":"138","endLine":43,"endColumn":38},"no-native-reassign",["142"],"no-negated-in-lhs",["143"],["142"],["143"],"@typescript-eslint/no-unused-vars","'temp_max_translated' is assigned a value but never used.","Identifier","unusedVar","'temp_min_translated' is assigned a value but never used.","'temp_units' is assigned a value but never used.","'zoom' is assigned a value but never used.","no-global-assign","no-uns