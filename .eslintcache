r } from '../../utils';\n\nimport { SecondaryContainer } from './secondary-info.styles';\n\nimport { WeatherData } from '../../store/types';\n\ninterface SecondaryProps extends WeatherData {\n  farengeit: boolean;\n}\n\nconst SecondaryInfo: FC<SecondaryProps> = (props) => {\n  //invoque class LocalTime, and initializing with data.timezone\n  let localTimeZone = 0;\n  if (props) localTimeZone = props.timezone * 1;\n  const localTime = new LocalTime(localTimeZone);\n  let windSpeed: string;\n  let temp_max_translated: string;\n  let temp_min_translated: string;\n  let speedUnits: string;\n  let temp_units: string;\n  if (props.farengeit) {\n    windSpeed = getMilesHour(props.wind.speed);\n    temp_max_translated = getFarengeit(props.main.temp_max);\n    temp_min_translated = getFarengeit(props.main.temp_min);\n    speedUnits = 'miles/h';\n    temp_units = 'ºF';\n  } else {\n    windSpeed = getKmHour(props.wind.speed);\n    temp_max_translated = getCelsius(props.main.temp_max);\n    temp_min_translated = getCelsius(props.main.temp_min);\n    speedUnits = 'km/h';\n    temp_units = 'ºC';\n  }\n\n  return (\n    <SecondaryContainer wind={props.wind.deg}>\n      <div className='secondary-item'>\n        <div className='wind-item'>\n          <h4>\n            WIND <p className='direction'>&#8593;&#8593;</p>\n          </h4>\n          <p>\n            {windSpeed} <span>{speedUnits}</span>\n          </p>\n        </div>\n      </div>\n      <div className='secondary-item'>\n        <h4>HUMIDITY</h4>\n        <p>\n          {props.main.humidity}\n          <span>%</span>\n        </p>\n      </div>\n      <div className='secondary-item'>\n        <h4>SUNRISE</h4>\n        <p className='p-small'>\n          {localTime.getTargetTime(props.sys.sunrise * 1)}\n        </p>\n      </div>\n      <div className='secondary-item'>\n        <h4>CLOUDS</h4>\n        <p>\n          {props.clouds.all}\n          <span>%</span>\n        </p>\n      </div>\n      <div className='secondary-item'>\n        <h4>PRESSURE</h4>\n        <p className='p-small'>\n          {props.main.pressure}\n          <span>Pa</span>\n        </p>\n      </div>\n      <div className='secondary-item'>\n        <h4>SUNSET</h4>\n        <p className='p-small'>\n          {localTime.getTargetTime(props.sys.sunset * 1)}\n        </p>\n      </div>\n    </SecondaryContainer>\n  );\n};\n\nexport default SecondaryInfo;\n","/home/pdr-clv/react/weather-ts/src/components/secondary-info/secondary-info.styles.tsx",[],"/home/pdr-clv/react/weather-ts/src/components/input-place/input-place.component.tsx",[],"/home/pdr-clv/react/weather-ts/src/components/input-place/input-place.styles.tsx",[],["126","127"],"/home/pdr-clv/react/weather-ts/src/store/types/forecastTypes.ts",[],"/home/pdr-clv/react/weather-ts/src/store/types/alertTypes.ts",[],"/home/pdr-clv/react/weather-ts/src/store/actions/forecastActions.ts",[],"/home/pdr-clv/react/weather-ts/src/store/reducers/forecastReducer.ts",[],{"ruleId":"128","replacedBy":"129"},{"ruleId":"130","replacedBy":"131"},{"ruleId":"132","severity":1,"message":"133","line":32,"column":5,"nodeType":"134","messageId":"135","endLine":32,"endColumn":24},{"ruleId":"132","severity":1,"message":"136","line":33,"column":5,"nodeType":"134","messageId":"135","endLine":33,"endColumn":24},{"ruleId":"132","severity":1,"message":"137","line":35,"column":5,"nodeType":"134","messageId":"135","endLine":35,"endColumn":15},{"ruleId":"128","replacedBy":"138"},{"ruleId":"130","replacedBy":"139"},"no-native-reassign",["140"],"no-negated-in-lhs",["141"],"@typescript-eslint/no-unused-vars","'temp_max_translated' is assigned a value but never used.","Identifier","unusedVar","'temp_min_translated' is assigned a value but never used.","'temp_units' is assigned a value but never used.",["140"],["141"],"no-global-assign","no-unsafe-negation"]