assName='secondary-item'>\n        <h4>HUMIDITY</h4>\n        <p>\n          {props.current.humidity}\n          <span>%</span>\n        </p>\n      </div>\n      <div className='secondary-item'>\n        <h4>SUNRISE</h4>\n        <p className='p-small'>\n          {localTime.getTargetTime(props.daily[0].sunrise)}\n        </p>\n      </div>\n      <div className='secondary-item'>\n        <h4>CLOUDS</h4>\n        <p>\n          {props.current.clouds}\n          <span>%</span>\n        </p>\n      </div>\n      <div className='secondary-item'>\n        <h4>PRESSURE</h4>\n        <p className='p-small'>\n          {props.current.pressure}\n          <span>Pa</span>\n        </p>\n      </div>\n      <div className='secondary-item'>\n        <h4>SUNSET</h4>\n        <p className='p-small'>\n          {localTime.getTargetTime(props.daily[0].sunset)}\n        </p>\n      </div>\n    </SecondaryContainer>\n  );\n};\n\nexport default SecondaryInfo;\n","/home/pdr-clv/react/weather-ts/src/components/secondary-info/secondary-info.styles.tsx",[],["140","141"],"/home/pdr-clv/react/weather-ts/src/components/input-place/input-place.component.tsx",["142"],"/home/pdr-clv/react/weather-ts/src/components/input-place/input-place.styles.tsx",[],"/home/pdr-clv/react/weather-ts/src/store/types/forecastTypes.ts",[],"/home/pdr-clv/react/weather-ts/src/store/types/alertTypes.ts",[],"/home/pdr-clv/react/weather-ts/src/store/actions/forecastActions.ts",[],"/home/pdr-clv/react/weather-ts/src/store/reducers/forecastReducer.ts",[],"/home/pdr-clv/react/weather-ts/src/components/forecast/forecast.component.tsx",[],"/home/pdr-clv/react/weather-ts/src/components/forecast/forecast.styles.tsx",[],{"ruleId":"143","replacedBy":"144"},{"ruleId":"145","replacedBy":"146"},{"ruleId":"143","replacedBy":"147"},{"ruleId":"145","replacedBy":"148"},{"ruleId":"143","replacedBy":"149"},{"ruleId":"145","replacedBy":"150"},{"ruleId":"151","severity":1,"message":"152","line":32,"column":5,"nodeType":"153","messageId